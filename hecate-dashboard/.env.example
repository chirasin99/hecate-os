# HecateOS Dashboard Configuration
# Copy this file to .env.local for local development

# Monitor WebSocket port (default: 9313)
# This should match the port configured in hecate-monitor
NEXT_PUBLIC_HECATE_MONITOR_PORT=9313

# Monitor host (for remote monitoring)
# NEXT_PUBLIC_HECATE_MONITOR_HOST=localhost

# Enable debug mode
# NEXT_PUBLIC_DEBUG=false